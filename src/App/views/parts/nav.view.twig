<nav class="menu">
    <ul>
        <li class="logo-item">
            <a href="/">
                <img src="./../../imagenes/logo.png" alt="logo">
            </a>
        </li>
        {% for item in menu %}
            {% if item.name == 'Acceso' %}
                <li class="botonLogin">
                    <a href="{{ item.href }}">
                        {{ item.name }}
                    </a>
                </li>
            {% else %}
                <li class="dropdown">
                    <a href="{{ item.href }}">
                        {{ item.name }}
                    </a>
                </li>
            {% endif %}
        {% endfor %}
    </ul>
</nav>
