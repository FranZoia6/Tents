{% extends "base.view.twig" %}

{% block main %}
<h1>{{ titulo }}</h1>
<section id="beachResortData" class="ticket">
  <label>Numero de reserva: {{ reservationData.fields.reservationId }}</label>
  <label>Balneario: {{ reservationData.fields.beachResortName }}</label>
  <label>Fecha desde: {{ reservationData.fields.from }}</label>
  <label>Fecha Hasta: {{ reservationData.fields.to }}</label>
    {% for unit in reservationData.fields.units %}
  <label>Unidad: {{ unit }}</label>
   {%endfor%}
  <label class="price-label">Precio:  ${{ reservationData.fields.reservationAmount }}</label>        

</section>

{% endblock main %}